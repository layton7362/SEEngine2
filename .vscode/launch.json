{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Main Debug ",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "f:/MyProgProject/OpenGLC++",
      "program": "f:/MyProgProject/OpenGLC++/build/Main.exe",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
